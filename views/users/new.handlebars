<form action="/users/create" method="POST">
    <input class="form-group" placeholder="Username" type="text" name="username"><br>
    <input class="form-group" placeholder="Email" type="email" name="email"><br>
    <input class="form-group" placeholder="Password" type="password" name="password"><br>
    <button class="btn" type="submit">Sign Up!</button>
</form>
