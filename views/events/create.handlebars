<div class="container">
<form name="upload" class="text-center" action="/events/create" method="POST" enctype="multipart/form-data">

  <h2>Enter event details:</h2>

  <div class="form-group">
		<label for="name">Name: </label>
    <input class=".col-lg-*" type="text" name="name" placeholder="Name">
  </div>

	<div class="form-group">
		<label for="djs">DJs: </label>
    <input class=".col-lg-*" type="text" name="djs" placeholder="DJs">
  </div>

  <div class="form-group">
		<label for="genre">Genre: </label>
    <input class=".col-lg-*" type="text" name="genre" placeholder="Genre">
  </div>

  <div class="form-group">
		<label for="Venue">Venue: </label>
    <input class=".col-lg-*" type="text" name="venue" placeholder="Venue">
  </div>

	<div class="form-group">
		<label for="Address">Address: </label>
    <input class=".col-lg-*" type="text" name="address" placeholder="Address">
  </div>

	<div class="form-group">
		<label for="Phone">Phone: </label>
		<input class=".col-lg-*" type="text" name="phone" placeholder="Phone">
	</div>

	<div class="form-group">
		<label for="Hours">Hours: </label>
		<input class=".col-lg-*" type="text" name="hours" placeholder="Hours">
	</div>

	<div class="form-group">
		<label for="Reservations">Reservations: </label>
		<input class=".col-lg-*" type="text" name="reservations" placeholder="Reservations">
	</div>

  <div class="form-group">
		<label for="image">Image: </label>
    <input id="imageUpload" type="file" name="image">
  </div>


  <button class="btn btn-theme" type="submit">Add Event</button>

</form>
